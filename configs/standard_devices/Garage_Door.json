      "bounds": {
        "min": 0,
        "max": 100,
        "step": 1
      },
      "ui_type": "esp.ui.slider"
    },
    {
      "name": "Obstruction Detected",
      "type": "esp.param.obstruction-detected",
      "properties": ["read"],
      "data_type": "bool",
      "default_value": false,
      "ui_type": "esp.ui.text"
    }
  ]
