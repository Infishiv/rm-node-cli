{
  "name": "Smart Plug",
  "type": "esp.device.plug",
  "params": [
    {
      "name": "Name",
      "type": "esp.param.name",
      "properties": ["read", "write"],
      "data_type": "string",
      "default_value": "Smart Plug",
      "bounds": {
        "min": 1,
        "max": 32,
        "step": 1
      }
    },
    {
      "name": "Power",
      "type": "esp.param.power",
      "properties": ["read", "write"],
      "data_type": "bool",
      "default_value": false,
      "ui_type": "esp.ui.toggle"
    },
    {
      "name": "Energy Consumption",
      "type": "esp.param.energy",
      "properties": ["read", "time_series"],
      "data_type": "float",
      "default_value": 0.0,
      "bounds": {
        "min": 0,
        "max": 9999,
        "step": 0.01
      },
      "ui_type": "esp.ui.text"
    }
  ]
}